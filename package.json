{
  "name": "maptoposter",
  "version": "1.0.0",
  "description": "Generate beautiful map posters for any city",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "maptoposter": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "generate:examples": "./scripts/generate-theme-examples.sh"
  },
  "keywords": [
    "map",
    "poster",
    "osm",
    "openstreetmap",
    "canvas"
  ],
  "license": "MIT",
  "dependencies": {
    "@rushstack/ts-command-line": "^5.1.5",
    "canvas": "^3.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.9",
    "eslint": "^9.39.2",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  }
}
