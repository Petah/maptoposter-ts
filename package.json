{
  "name": "maptoposter",
  "version": "1.0.0",
  "description": "Generate beautiful map posters for any city",
  "main": "dist/index.js",
  "bin": {
    "maptoposter": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "map",
    "poster",
    "osm",
    "openstreetmap",
    "canvas"
  ],
  "license": "MIT",
  "dependencies": {
    "@rushstack/ts-command-line": "^5.1.5",
    "canvas": "^3.2.1",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.9",
    "eslint": "^9.39.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  }
}
